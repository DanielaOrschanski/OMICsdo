git config --global user.name "DanielaOrschanski"
git config --global user.email "dorschanski@gmail.com"

library(devtools)
usethis::use_git()
usethis::use_git_config(user.name = "DanielaOrschanski", user.email = "dorschanski@gmail.com")
usethis::use_git_remote(name = "origin", url = "https://github.com/DanielaOrschanski/OMICsdo.git")

system("git add .")
system('git commit -m "Primer commit"')
